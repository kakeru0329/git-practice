<h1>ジャンル一覧・追加</h1>


<div>
  <%= form_with model: @genre,url: admin_genres_path,method: :post, remote: true do |f| %>
    <%= f.label :name, "ジャンル名" %>
    <%= f.text_field :name, id: "form" %>
    <%= f.submit "追加する" %>
  <% end %>
  <table class="table">
    <thead>
      <tr>
        <th>ジャンル</th>
        <th></th>
      </tr>
    </thead>

    <tbody>

    </tbody>
  </table>

</div>